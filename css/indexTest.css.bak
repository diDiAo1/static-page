/*时间轴*/
.cbp_tmtimeline {
    margin: 0 0 0 100px;
    padding: 0;
    list-style: none;
    position: relative;
}

.cbp_tmtimeline > li {
    position: relative;
}

.cbp_tmtimeline > li .cbp_tmtime {
    display: block;
    width: 25%;
    padding-right: 100px;
    position: absolute;
}

/*显示时间文字*/
.cbp_tmtimeline > li .cbp_tmtime span:first-child {
    font-size: 1.3em;
    color: #4cae4c;
}
.cbp_tmtimeline > li .cbp_tmtime span {
    display: block;
    text-align: right;
}

/*时间轴节点显示圆圈*/
.cbp_tmtimeline > li .cbp_tmicon {
    width: 40px;
    height: 40px;
    font-family: 'ecoico';
    speak: none;
    font-style: normal;
    font-weight: normal;
    font-variant: normal;
    text-transform: none;
    font-size: 1.2em;
    line-height: 40px;
    -webkit-font-smoothing: antialiased;
    position: absolute;
    color: #fff;
    background-image: linear-gradient(to bottom,#eee 0,#ddd 100%);
    border-radius: 50%;
    box-shadow: 0 1px 6px #ddd;
    text-align: center;
    left: 20%;
    top: 0;
    margin: 0 0 0 -25px;
}

.cbp_tmicon span {
	text-align:center;
	margin-top:10px;
}

/*时间轴右侧显示内容*/
.cbp_tmtimeline > li .cbp_tmlabel {
    margin: 0 0 30px 25%;
    background: #eee;
    padding: 1em;
    font-size: 1.2em;
    font-weight: 300;
    line-height: 1.4;
    position: relative;
    border-radius: 5px;
}

/*时间轴纵轴竖线*/
.cbp_tmtimeline:before {
    content: '';
    position: absolute;
    top: 0;
    bottom: 0;
    width: 5px;
    box-shadow: 0 1px 6px rgba(0,0,0,.175);
    background-color: #ddd;
    left: 20%;
    margin-left: -7.5px;
}


/*时间轴右侧箭头*/
.cbp_tmtimeline > li .cbp_tmlabel:after {
    right: 100%;
    border: solid transparent;
    content: " ";
    height: 0;
    width: 0;
    position: absolute;
    border-right-color: #eee;
    border-width: 10px;
    top: 10px;
}

